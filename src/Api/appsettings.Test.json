{
  "ConnectionStrings": {
    "DefaultConnection": "DEFAULT_CONNECTION"
  },
  "Zeebe": {
    "ModelFilename": "ContractApproval.bpmn",
    "ZeebeGateway": "ZEEBE_GATEWAY"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "MinimumLogEventLevel": "Debug"
    },
    "Elasticsearch": {
      "IsEnabled": false,
      "Host": "ELASTIC_SEARCH",
      "IndexFormat": "mslogs-{0:yyyy.MM.dd}",
      "MinimumLogEventLevel": "Debug"
    }
  },
  "Entegration": {
    "Internals": "INTERNALS",
    "TemplateEngine": "TEMPLATE_ENGINE",
    "Sso": "SSO",
    "MessagingGateway": "MESSAGING_GATEWAY",
    "ClientSecret": "CLIENT_SECRET",
    "ApiClientSecret": "API_CLIENT_SECRET",
    "ClientId": "CLIENT_ID"
  },
  "ServiceEndpoint": {
    "DMSService": "DMSSERVICE"
  },
  "Authentication": {
    "Authority": "AUTHORITY",
    "ApiGateway": "API_GATEWAY",
    "RedirectUri": "REDIRECT_URI"
  },
  "Token": {
    "Issuer": "Issuer",
    "Audience": "Audience",
    "SecurityKey": "SECURITYKEY"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {

          "connectionString": "LOG_DB",
          "sinkOptionsSection": {
            "tableName": "Logs",
            "schemaName": "Endorsement",
            "autoCreateSqlTable": true,
            "batchPostingLimit": 1000,
            "period": "0.00:00:30"
          },
          "columnOptionsSection": {
            "customColumns": [
              {
                "ColumnName": "OrderId",
                "DataType": "nvarchar",
                "DataLength": 40,
                "AllowNull": true
              },
              {
                "ColumnName": "UserName",
                "DataType": "nvarchar",
                "DataLength": 256,
                "AllowNull": true

              }
            ]
          }
        }
      }
    ]
  }
}
